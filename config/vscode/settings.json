{
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.emptySelectionClipboard": true,
  "editor.fontFamily": "Source Code Pro",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.rulers": [80],
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": false,
  "keyboard.touchbar.enabled": false,
  "npm.fetchOnlinePackageInfo": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "typescript.disableAutomaticTypeAcquisition": true,
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Primer Light",
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.statusBar.feedback.visible": false,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontFamily": "SourceCodePro-Semibold",
  "workbench.colorCustomizations": {
    "[Primer Light]": {
      "terminal.background": "#fafafa",
      "terminal.foreground": "#373a42",
      "terminal.ansiBlack": "#000000",
      "terminal.ansiRed": "#e65544",
      "terminal.ansiGreen": "#4ca24a",
      "terminal.ansiYellow": "#dbc18e",
      "terminal.ansiBlue": "#3c74f5",
      "terminal.ansiMagenta": "#a726a5",
      "terminal.ansiCyan": "#4ca24a",
      "terminal.ansiWhite": "#c7c7c7",
      "terminal.ansiBrightBlack": "#000000",
      "terminal.ansiBrightRed": "#e65544",
      "terminal.ansiBrightGreen": "#4ca24a",
      "terminal.ansiBrightYellow": "#dbc18e",
      "terminal.ansiBrightBlue": "#3c74f5",
      "terminal.ansiBrightMagenta": "#b127a5",
      "terminal.ansiBrightCyan": "#4ca24a",
      "terminal.ansiBrightWhite": "#feffff",
    }
  }
}
